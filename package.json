{
  "name": "napi-spellchecker",
  "version": "0.1.9",
  "description": "Spell Checker built against ABI-stable Node.js API",
  "main": "./lib/spellchecker.js",
  "gypfile": true,
  "dependencies": {
    "bindings": "~1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/rebornix/napi-spellchecker.git"
  },
  "binary": {
    "napi_versions": [
      2,
      3
    ]
  },
  "devDependencies": {
    "prebuild": "^8.2.1",
    "jasmine-focused": "1.x",
    "tar": ">=4.4.2"
